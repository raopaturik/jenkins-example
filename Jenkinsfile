pipeline {
    agent none

    stages {

        stage ('Build') {
            agent any

            steps {
                echo 'Build Completed'
            }
        }

        stage ('Deploy') {
            agent any

            steps {
                echo 'Deploy Completed'
            }
        }

        stage ('Test') {
            agent any

            steps {
                echo 'Test Completed'
            }
        }

        stage ('Release') {
            agent any

            steps {
                echo 'Release Completed'
            }
        }

    }
}
